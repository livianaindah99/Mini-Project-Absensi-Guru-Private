<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:cardBackgroundColor="#F8E969"
        app:cardCornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/nim_siswa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="NIM Siswa Terpilih"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                android:layout_margin="7dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/nama_siswa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama Siswa Terpilih"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@+id/nim_siswa"
                android:layout_margin="7dp"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/kls_siswa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kelas Siswa Terpilih"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@+id/nama_siswa"
                android:layout_margin="7dp"
                app:layout_constraintStart_toStartOf="parent"/>


            <TextView
                android:id="@+id/tittle_jam_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Jam Login"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@+id/kls_siswa"
                android:layout_margin="7dp"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/tittle_jam_logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Jam Logout"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                android:layout_margin="7dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tittle_jam_login" />

            <TextView
                android:id="@+id/tittle_tanggal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tanggal"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                android:layout_margin="7dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tittle_jam_logout" />

            <TextView
                android:id="@+id/tittle_lokasi_latitude"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lokasi Latitude"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tittle_tanggal" />

            <TextView
                android:id="@+id/tittle_lokasi_longitude"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lokasi Longitude"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/tittle_lokasi_latitude"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <TextView
                android:id="@+id/txt_nimssw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>

            <TextView
                android:id="@+id/txt_namassw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@id/txt_nimssw"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/txt_kelasssw"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="3"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@id/txt_namassw"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/txt_jam_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="4"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@id/txt_kelasssw"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/txt_jam_logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@id/txt_jam_login"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/txt_tanggal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="6"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@id/txt_jam_logout"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/txt_lokasi_latitude"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="7"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@id/txt_tanggal"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/txt_lokasi_longitude"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="8"
                android:layout_margin="7dp"
                android:textSize="9pt"
                android:fontFamily="@font/boogalo_regular"
                app:layout_constraintTop_toBottomOf="@id/txt_lokasi_latitude"
                app:layout_constraintEnd_toEndOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>